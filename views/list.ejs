    <%- include('header') -%>
    <div class="box" id="heading">
        <h1><%= listType %></h1>
    </div>
    <div class="box">
            <% for(var i = 0; i < newListItems.length; i++){ %>
                <div class="item">
                <input type="checkbox">
                <p><%= newListItems[i] %></p>
            </div>
            <% } %>
            <form class="item" action="<%= route %>" method="post">
                <input type="text" name="newItem" id="" placeholder="New Item" autocomplete="off" autofocus>
                <button type="submit">+</button>
            </form>
</div>
    
<%- include('footer') -%>